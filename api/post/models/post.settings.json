{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "excerpt": {
      "type": "text"
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "date": {
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "author": {
      "model": "author"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "published",
        "draft"
      ]
    },
    "category": {
      "via": "posts",
      "model": "category"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "cta.button",
        "body.section",
        "marketing.subscribe"
      ]
    },
    "meta": {
      "type": "component",
      "repeatable": true,
      "component": "seo.meta"
    },
    "herocta": {
      "type": "component",
      "repeatable": false,
      "component": "cta.button"
    }
  }
}
